@model List<MusicTrackViewModel>


<body>

    <table class="table-bordered">
        <caption>لیست آهنگ ها</caption>
        <tr>
            <th>نام آهنگ</th>
            <th>خواننده</th>
            <th>شاعر</th>
            <th>آلبوم</th>
            <th>رسته آهنگ</th>
            <th>توضیحات</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
                <th>@item.TitleMusic</th>
                <th>@item.Vocalist</th>
                <th>@item.Songwriter</th>
                <th>@item.Album</th>
                <th>@item.MusicCategory</th>
                <th>@item.Description</th>
                <th><a asp-action="Details" asp-route-id="@item.Id">جزئیات</a></th>
                <th><a asp-action="Update" asp-route-id="@item.Id">ویرایش</a></th>
                <th><a asp-action="Remove" asp-route-id="@item.Id">حذف</a></th>

                @* <th><button asp-action="Update" class="btn-dark" asp-route-id="@item.Id">ویرایش</button></th>
                <th><button asp-action="Delete" class="btn-danger" asp-route-id="@item.Id">حذف</button></th>*@
             
        </tr>
        }
    </table>
